{
  "name": "dicty-components-page-editor",
  "version": "0.0.0-semantically-released",
  "description": "dictyBase page editor built using Slatejs",
  "author": "wildlifehexagon",
  "module": "dist/dicty-components-page-editor.esm.js",
  "license": "BSD-2-Clause",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dictyBase/dicty-components-page-editor.git"
  },
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests --watch",
    "test-coverage": "tsdx test --passWithNoTests --coverage",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "semantic-release": "semantic-release",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run"
  },
  "dependencies": {
    "@fontsource/lato": "^4.2.2",
    "@fontsource/merriweather": "^4.2.2",
    "@fontsource/montserrat": "^4.2.2",
    "@fontsource/roboto": "^4.2.3",
    "@fontsource/roboto-condensed": "^4.2.2",
    "@fontsource/roboto-mono": "^4.2.2",
    "@fontsource/roboto-slab": "^4.2.2",
    "react-colorful": "^5.1.4",
    "slate": "^0.59.0",
    "slate-history": "^0.59.0",
    "slate-react": "^0.59.0"
  },
  "peerDependencies": {
    "@material-ui/core": ">=4.0.0",
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "devDependencies": {
    "@material-ui/core": "^4.11.4",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/github": "^7.2.1",
    "@semantic-release/npm": "^7.1.1",
    "@semantic-release/release-notes-generator": "^9.0.2",
    "@testing-library/cypress": "^7.0.6",
    "@testing-library/dom": "^7.30.4",
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^11.2.6",
    "@testing-library/user-event": "^12.6.3",
    "css-loader": "^5.2.4",
    "cypress": "^7.2.0",
    "file-loader": "^6.2.0",
    "husky": "^4.3.6",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "react": "^16.14.0",
    "react-docgen-typescript": "^1.22.0",
    "react-dom": "^16.14.0",
    "react-styleguidist": "^11.1.5",
    "react-test-renderer": "^16.14.0",
    "semantic-release": "^17.4.2",
    "slate-hyperscript": "^0.59.0",
    "source-map-explorer": "^2.5.1",
    "style-loader": "^2.0.0",
    "ts-loader": "^8.1.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.2.0",
    "typescript": "^4.2.4",
    "url-loader": "^4.1.1",
    "webpack": "4"
  },
  "files": [
    "dist"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "prettier --no-semi --trailing-comma all --jsx-bracket-same-line true --write",
      "git add"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*",
      "!**/*test*/**",
      "!**/*fixture*/**",
      "!src/components/icons/**",
      "!src/utils/**"
    ]
  }
}
