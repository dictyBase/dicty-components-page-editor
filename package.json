{
  "name": "dicty-components-page-editor",
  "version": "0.0.0-semantically-released",
  "description": "dictyBase page editor built using Slatejs",
  "author": "wildlifehexagon",
  "module": "dist/dicty-components-page-editor.esm.js",
  "license": "BSD-2-Clause",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dictyBase/dicty-components-page-editor.git"
  },
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "test-coverage": "tsdx test --passWithNoTests --coverage",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "semantic-release": "semantic-release",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build"
  },
  "dependencies": {
    "slate": "^0.59.0",
    "slate-react": "^0.59.0"
  },
  "peerDependencies": {
    "@material-ui/core": ">=4.0.0",
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "devDependencies": {
    "@material-ui/core": "^4.11.2",
    "@semantic-release/git": "^9.0.0",
    "husky": "^4.3.6",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-styleguidist": "^11.1.5",
    "react-test-renderer": "^16.14.0",
    "semantic-release": "^17.3.1",
    "slate-hyperscript": "^0.59.0",
    "source-map-explorer": "^2.5.1",
    "tsdx": "^0.14.1",
    "tslib": "^2.1.0",
    "typescript": "^4.1.3"
  },
  "files": [
    "dist"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "prettier --no-semi --trailing-comma all --jsx-bracket-same-line true --write",
      "git add"
    ]
  }
}
